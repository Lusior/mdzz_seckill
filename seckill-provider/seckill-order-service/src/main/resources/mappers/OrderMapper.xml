<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.shuai.seckill.mapper.dao.OrderMapper">

    <select id="selectByUserId" resultType="com.shuai.seckill.entity.Order">
        select *
        from tb_order
        where user_id = #{userId}
    </select>

</mapper>